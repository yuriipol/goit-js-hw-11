{{#each this}}
  <div class='photo-card'>
    <a href='{{this.largeImageURL}}'><img
        src='{{this.webformatURL}}'
        alt='{{this.tags}}'
        loading='lazy'
        width='200'
        height='150'
      /></a>
    <div class='info'>
      <p class='info-item'>
        <b class='text-card'>Likes: {{this.likes}}</b>
      </p>
      <p class='info-item'>
        <b class='text-card'>Views: {{this.views}}</b>
      </p>
      <p class='info-item'>
        <b class='text-card'>Comments: {{this.comments}}</b>
      </p>
      <p class='info-item'>
        <b class='text-card'>Downloads: {{this.downloads}} </b>
      </p>
    </div>
  </div>
{{/each}}